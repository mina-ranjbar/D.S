#Example 6
Listx = ["math","physics","arabic"]
print(Listx)

Listx.append("Algebra")
print(Listx)

Listx.append(45)
print(Listx)

Listx.remove(45)
print(Listx)

Listx = ["math","physics","arabic"]
print(Listx.index("math"))

print(Listx.count("math"))

print(Listx.count("algebra"))


Listx.extend(Listx)
print(Listx)

Listx.extend(["English","chemistry"])
print(Listx)

Listx.insert(4,"farsi")
print(Listx)

print(len(Listx))

Listy = [1,4,9,2,3,7]
Listy.sort()
print(Listy)

Listz = ["d","a","z","s"]
Listz.sort()
print(Listz)

#output:
['math', 'physics', 'arabic']
['math', 'physics', 'arabic', 'Algebra']
['math', 'physics', 'arabic', 'Algebra', 45]
['math', 'physics', 'arabic', 'Algebra']
