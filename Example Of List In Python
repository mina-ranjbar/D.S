#Example 1
List = ["python",12,"matlab",45]
print(List)

List1 = ["Day","12",23,"#","de45","rt%","56!"]
print(List1)

print(List1[:])
print(List1[1])
print(List1[2])
print(List1[1:3])
print(List1[2:])
print(List1[2:2])
print(List1[2:11])

Length = len(List1)
print(Length)
print(List1[Length - 1])
print(List1[len(List1) - 1])

List = ["python",12,"matlab",45]
print(List)

List1 = ["Day","12",23,"#","de45","rt%","56!"]
print(List1)

#output:
['python', 12, 'matlab', 45]
['Day', '12', 23, '#', 'de45', 'rt%', '56!']
['Day', '12', 23, '#', 'de45', 'rt%', '56!']
