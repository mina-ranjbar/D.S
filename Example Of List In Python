#Example 2
Day = ["saturday","sunday","monday","tuesday","wednesday"]
len(Day)
count = 0
print("Today is: ", Day[count])

count = 3
print("Today is: ", Day[count])

Day1 = [1394,"30",["Dey","Bahman"],"aban",3.14]
print(len(Day1))

print(Day1[2])
print(Day1[3])
print(Day1[3][1])
print(Day1[2][1][2])
print(Day1[-3])
print(Day1[-3][1][-4])

#ouput:
